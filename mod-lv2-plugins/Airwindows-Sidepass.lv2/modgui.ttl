@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://hannesbraun.net/ns/lv2/airwindows/sidepass>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-sidepass.html> ;
        modgui:stylesheet <modgui/stylesheet-sidepass.css> ;
        modgui:screenshot <modgui/screenshot-sidepass.png> ;
        modgui:thumbnail <modgui/thumbnail-sidepass.png> ;
        modgui:brand "Hannes" ;
        modgui:label "Sidepass" ;
        modgui:model "boxy" ;
        modgui:panel "1-knob" ;
        modgui:color "darkblue" ;
        modgui:knob "aluminium" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "cutoff" ;
            lv2:name "Cutoff" ;
        ] ;
    ] .