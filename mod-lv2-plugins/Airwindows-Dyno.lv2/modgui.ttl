@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://hannesbraun.net/ns/lv2/airwindows/dyno>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-dyno.html> ;
        modgui:stylesheet <modgui/stylesheet-dyno.css> ;
        modgui:screenshot <modgui/screenshot-dyno.png> ;
        modgui:thumbnail <modgui/thumbnail-dyno.png> ;
        modgui:brand "Hannes" ;
        modgui:label "Dyno" ;
        modgui:model "boxy" ;
        modgui:panel "1-knob" ;
        modgui:color "brown" ;
        modgui:knob "aluminium" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "input" ;
            lv2:name "Input" ;
        ] ;
    ] .