@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix mod:  <http://moddevices.com/ns/mod#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rsz:  <http://lv2plug.in/ns/ext/resize-port#> .
@prefix unit: <http://lv2plug.in/ns/extensions/units#> .

<urn:zamaudio:ZamGate>
    a lv2:Plugin ;

    lv2:extensionData <http://lv2plug.in/ns/ext/state#interface> ,
                      <http://kxstudio.sf.net/ns/lv2ext/programs#Interface> ;

    lv2:optionalFeature <http://lv2plug.in/ns/lv2core#hardRTCapable> ,
                        <http://lv2plug.in/ns/ext/buf-size#boundedBlockLength> ;

    lv2:requiredFeature <http://lv2plug.in/ns/ext/options#options> ,
                        <http://lv2plug.in/ns/ext/urid#map> ;

    lv2:port [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 0 ;
        lv2:symbol "lv2_audio_in_1" ;
        lv2:name "Audio Input 1" ;
    ] ,
    [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 1 ;
        lv2:symbol "lv2_sidechain_in" ;
        lv2:name "Sidechain Input" ;
        lv2:portProperty lv2:isSideChain;
    ] ;

    lv2:port [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 2 ;
        lv2:symbol "lv2_audio_out_1" ;
        lv2:name "Audio Output 1" ;
    ] ;

    lv2:port [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 3 ;
        lv2:name "Attack" ;
        lv2:symbol "att" ;
        lv2:default 50.000000 ;
        lv2:minimum 0.100000 ;
        lv2:maximum 500.000000 ;
        unit:unit unit:ms ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 4 ;
        lv2:name "Release" ;
        lv2:symbol "rel" ;
        lv2:default 100.000000 ;
        lv2:minimum 0.100000 ;
        lv2:maximum 500.000000 ;
        unit:unit unit:ms ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 5 ;
        lv2:name "Threshold" ;
        lv2:symbol "thr" ;
        lv2:default -60.000000 ;
        lv2:minimum -60.000000 ;
        lv2:maximum 0.000000 ;
        unit:unit unit:db ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 6 ;
        lv2:name "Makeup" ;
        lv2:symbol "mak" ;
        lv2:default 0.000000 ;
        lv2:minimum -30.000000 ;
        lv2:maximum 30.000000 ;
        unit:unit unit:db ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 7 ;
        lv2:name "Sidechain" ;
        lv2:symbol "sidechain" ;
        lv2:default 0.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 1.000000 ;
        unit:unit [
            rdfs:label  " " ;
            unit:symbol " " ;
            unit:render "%f  " ;
        ] ;
        lv2:portProperty lv2:toggled ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 8 ;
        lv2:name "Max gate close" ;
        lv2:symbol "close" ;
        lv2:default -50.000000 ;
        lv2:minimum -50.000000 ;
        lv2:maximum 0.000000 ;
        unit:unit unit:db ;
    ] ,
    [
        a lv2:OutputPort, lv2:ControlPort ;
        lv2:index 9 ;
        lv2:name "Output Level" ;
        lv2:symbol "outlevel" ;
        lv2:default -45.000000 ;
        lv2:minimum -45.000000 ;
        lv2:maximum 20.000000 ;
        unit:unit unit:db ;
    ] ,
    [
        a lv2:OutputPort, lv2:ControlPort ;
        lv2:index 10 ;
        lv2:name "Gain Reduction" ;
        lv2:symbol "gainr" ;
        lv2:default 0.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 40.000000 ;
        unit:unit unit:db ;
    ] ;

    rdfs:comment """
Gate plugin for ducking low gain sounds.
""" ;

    mod:brand "ZamAudio" ;
    mod:label "ZamGate" ;

    doap:name "ZamGate" ;
    doap:license "GPL v2+" ;

    doap:maintainer [
        foaf:name "Damien Zammit" ;
        foaf:homepage <http://www.zamaudio.com> ;
    ] ;

    lv2:microVersion 7 ;
    lv2:minorVersion 2 .

