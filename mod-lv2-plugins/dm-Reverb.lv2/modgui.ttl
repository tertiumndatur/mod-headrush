@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://github.com/davemollen/dm-Reverb>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-dm-reverb.html> ;
        modgui:stylesheet <modgui/stylesheet-dm-reverb.css> ;
        modgui:screenshot <modgui/screenshot-dm-reverb.png> ;
        modgui:thumbnail <modgui/thumbnail-dm-reverb.png> ;
        modgui:javascript <modgui/javascript.js> ;
        modgui:brand "DM" ;
        modgui:label "Reverb" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "size" ;
            lv2:name "Size" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "predelay" ;
            lv2:name "Predelay" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "reverse" ;
            lv2:name "Reverse" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "speed" ;
            lv2:name "Speed" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "depth" ;
            lv2:name "Depth" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "absorb" ;
            lv2:name "Absorb" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "decay" ;
            lv2:name "Decay" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "tilt" ;
            lv2:name "Tilt" ;
        ] , [
            lv2:index 8 ;
            lv2:symbol "shimmer" ;
            lv2:name "Shimmer" ;
        ] , [
            lv2:index 9 ;
            lv2:symbol "mix" ;
            lv2:name "Mix" ;
        ] ;
    ] .