@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix mod:  <http://moddevices.com/ns/mod#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.

<http://moddevices.com/plugins/caps/CEO>
a lv2:OscillatorPlugin, lv2:Plugin;
lv2:microVersion 26;
lv2:minorVersion 9;
lv2:optionalFeature lv2:hardRTCapable;
mod:brand "CAPS";
mod:label "CEO";

rdfs:comment """
The Chief Executive Oscillator forever calls for more profit.

Sound data created with the flite[flite] application.

source: http://quitte.de/dsp/caps.html#CEO
""";

lv2:port
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 0;
    lv2:default 80.0;
    lv2:maximum 232.0;
    lv2:minimum 30.0;
    lv2:name "Profit per minute";
    lv2:symbol "ppm";
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 1;
    lv2:default 0.75;
    lv2:maximum 1.0;
    lv2:minimum 0.0;
    lv2:name "Volume";
    lv2:symbol "volume";
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 2;
    lv2:default 0.0;
    lv2:maximum 1.0;
    lv2:minimum 0.0;
    lv2:name "Damping";
    lv2:symbol "damping";
],
[
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 3;
    lv2:name "Out";
    lv2:symbol "out"
];

doap:license <http://usefulinc.com/doap/licenses/gpl>;

doap:developer [
    foaf:homepage <http://quitte.de/dsp/caps.html>;
    foaf:mbox <mailto:tim@quitte.de>;
    foaf:name "Tim Goetze"
];

doap:maintainer [
    foaf:homepage <http://moddevices.com>;
    foaf:mbox <mailto:devel@moddevices.com>;
    foaf:name "MOD Team"
];

doap:name "C* CEO - Chief Executive Oscillator".
