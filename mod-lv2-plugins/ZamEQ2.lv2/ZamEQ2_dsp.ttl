@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix mod:  <http://moddevices.com/ns/mod#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rsz:  <http://lv2plug.in/ns/ext/resize-port#> .
@prefix unit: <http://lv2plug.in/ns/extensions/units#> .

<urn:zamaudio:ZamEQ2>
    a lv2:Plugin ;

    lv2:extensionData <http://lv2plug.in/ns/ext/state#interface> ,
                      <http://kxstudio.sf.net/ns/lv2ext/programs#Interface> ;

    lv2:optionalFeature <http://lv2plug.in/ns/lv2core#hardRTCapable> ,
                        <http://lv2plug.in/ns/ext/buf-size#boundedBlockLength> ;

    lv2:requiredFeature <http://lv2plug.in/ns/ext/options#options> ,
                        <http://lv2plug.in/ns/ext/urid#map> ;

    lv2:port [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 0 ;
        lv2:symbol "lv2_audio_in_1" ;
        lv2:name "Audio Input 1" ;
    ] ;

    lv2:port [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 1 ;
        lv2:symbol "lv2_audio_out_1" ;
        lv2:name "Audio Output 1" ;
    ] ;

    lv2:port [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 2 ;
        lv2:name "Boost/Cut 1" ;
        lv2:symbol "boost1" ;
        lv2:default 0.000000 ;
        lv2:minimum -50.000000 ;
        lv2:maximum 20.000000 ;
        unit:unit unit:db ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 3 ;
        lv2:name "Bandwidth 1" ;
        lv2:symbol "bw1" ;
        lv2:default 1.000000 ;
        lv2:minimum 0.100000 ;
        lv2:maximum 6.000000 ;
        unit:unit [
            rdfs:label  " " ;
            unit:symbol " " ;
            unit:render "%f  " ;
        ] ;
        lv2:portProperty <http://lv2plug.in/ns/ext/port-props#logarithmic> ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 4 ;
        lv2:name "Frequency 1" ;
        lv2:symbol "f1" ;
        lv2:default 500.000000 ;
        lv2:minimum 20.000000 ;
        lv2:maximum 14000.000000 ;
        unit:unit unit:hz ;
        lv2:portProperty <http://lv2plug.in/ns/ext/port-props#logarithmic> ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 5 ;
        lv2:name "Boost/Cut 2" ;
        lv2:symbol "boost2" ;
        lv2:default 0.000000 ;
        lv2:minimum -50.000000 ;
        lv2:maximum 20.000000 ;
        unit:unit unit:db ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 6 ;
        lv2:name "Bandwidth 2" ;
        lv2:symbol "bw2" ;
        lv2:default 1.000000 ;
        lv2:minimum 0.100000 ;
        lv2:maximum 6.000000 ;
        unit:unit [
            rdfs:label  " " ;
            unit:symbol " " ;
            unit:render "%f  " ;
        ] ;
        lv2:portProperty <http://lv2plug.in/ns/ext/port-props#logarithmic> ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 7 ;
        lv2:name "Frequency 2" ;
        lv2:symbol "f2" ;
        lv2:default 3000.000000 ;
        lv2:minimum 20.000000 ;
        lv2:maximum 14000.000000 ;
        unit:unit unit:hz ;
        lv2:portProperty <http://lv2plug.in/ns/ext/port-props#logarithmic> ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 8 ;
        lv2:name "Boost/Cut L" ;
        lv2:symbol "boostl" ;
        lv2:default 0.000000 ;
        lv2:minimum -50.000000 ;
        lv2:maximum 20.000000 ;
        unit:unit unit:db ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 9 ;
        lv2:name "Frequency L" ;
        lv2:symbol "fl" ;
        lv2:default 250.000000 ;
        lv2:minimum 20.000000 ;
        lv2:maximum 14000.000000 ;
        unit:unit unit:hz ;
        lv2:portProperty <http://lv2plug.in/ns/ext/port-props#logarithmic> ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 10 ;
        lv2:name "Boost/Cut H" ;
        lv2:symbol "boosth" ;
        lv2:default 0.000000 ;
        lv2:minimum -50.000000 ;
        lv2:maximum 20.000000 ;
        unit:unit unit:db ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 11 ;
        lv2:name "Frequency H" ;
        lv2:symbol "fh" ;
        lv2:default 8000.000000 ;
        lv2:minimum 20.000000 ;
        lv2:maximum 14000.000000 ;
        unit:unit unit:hz ;
        lv2:portProperty <http://lv2plug.in/ns/ext/port-props#logarithmic> ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 12 ;
        lv2:name "Master Gain" ;
        lv2:symbol "master" ;
        lv2:default 0.000000 ;
        lv2:minimum -12.000000 ;
        lv2:maximum 12.000000 ;
        unit:unit unit:db ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 13 ;
        lv2:name "Peaks ON" ;
        lv2:symbol "peaks" ;
        lv2:default 0.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 1.000000 ;
        unit:unit [
            rdfs:label  " " ;
            unit:symbol " " ;
            unit:render "%f  " ;
        ] ;
        lv2:portProperty lv2:toggled ;
    ] ;

    rdfs:comment """
Two band parametric equaliser with high and low shelving circuits.
""" ;

    mod:brand "ZamAudio" ;
    mod:label "ZamEQ2" ;

    doap:name "ZamEQ2" ;
    doap:license "GPL v2+" ;

    doap:maintainer [
        foaf:name "Damien Zammit" ;
        foaf:homepage <http://www.zamaudio.com> ;
    ] ;

    lv2:microVersion 7 ;
    lv2:minorVersion 2 .

