@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://guitarix.sourceforge.net/plugins/gx_shakatube_#_shakatube_>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-gxshakatube.html> ;
        modgui:stylesheet <modgui/stylesheet-gxshakatube.css> ;
        modgui:screenshot <modgui/screenshot-gxshakatube.png> ;
        modgui:thumbnail <modgui/thumbnail-gxshakatube.png> ;
        modgui:brand " Guitarix " ;
        modgui:label "GxShakaTube" ;
        modgui:model "boxy" ;
        modgui:panel "3-knobs" ;
        modgui:color "black" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "DRIVE" ;
            lv2:name "DRIVE" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "TONE" ;
            lv2:name "TONE" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "VOLUME" ;
            lv2:name "VOLUME" ;
        ] ;
    ] .
