@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix mod: <http://moddevices.com/ns/mod#>.

<http://moddevices.com/plugins/tap/doubler>
a lv2:Plugin, lv2:SpectralPlugin;

doap:name "TAP Fractal Doubler";

doap:developer [
    foaf:name "Tom Szilagyi";
    foaf:homepage <http://tap-plugins.sourceforge.net/>;
    foaf:mbox <mailto:tsziagyi@users.sourceforge.net>;
    ];

doap:maintainer [
    foaf:name "MOD Team";
    foaf:homepage <http://moddevices.com>;
    foaf:mbox <mailto:devel@moddevices.com>;
    ];

mod:brand "TAP";
mod:label "Fractal Doubler";

doap:license <http://usefulinc.com/doap/licenses/gpl>;
lv2:minorVersion 7;
lv2:microVersion 2;

rdfs:comment """
Originally developed to do vocal doubling, this plugin is suitable for doubling tracks with vocals, acoustic/electric guitars, bass and just about any other instrument on them. The effect is created by applying small changes to the pitch and timing of the incoming signal. These changes are created by one-dimensional random fractal lines producing pink noise.

source: http://tap-plugins.sourceforge.net/ladspa/doubler.html
""";

lv2:port
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 0;
    lv2:symbol "TimeTracking";
    lv2:name "Time Tracking";
    lv2:default 0.50;
    lv2:minimum 0;
    lv2:maximum 1;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 1;
    lv2:symbol "PitchTracking";
    lv2:name "Pitch Tracking";
    lv2:default 0.50;
    lv2:minimum 0;
    lv2:maximum 1;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 2;
    lv2:symbol "DryLevelDb";
    lv2:name "Dry Level";
    lv2:default 0;
    lv2:minimum -90;
    lv2:maximum 20;
    units:unit units:db;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 3;
    lv2:symbol "DryLeftPosition";
    lv2:name "Dry Left Position";
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 1;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 4;
    lv2:symbol "DryRightPosition";
    lv2:name "Dry Right Position";
    lv2:default 1;
    lv2:minimum 0;
    lv2:maximum 1;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 5;
    lv2:symbol "WetLevelDb";
    lv2:name "Wet Level";
    lv2:default 0;
    lv2:minimum -90;
    lv2:maximum 20;
    units:unit units:db;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 6;
    lv2:symbol "WetLeftPosition";
    lv2:name "Wet Left Position";
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 1;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 7;
    lv2:symbol "WetRightPosition";
    lv2:name "Wet Right Position";
    lv2:default 1;
    lv2:minimum 0;
    lv2:maximum 1;
],
[
    a lv2:InputPort, lv2:AudioPort;
    lv2:index 8;
    lv2:symbol "Input_L";
    lv2:name "Input_L";
],
[
    a lv2:InputPort, lv2:AudioPort;
    lv2:index 9;
    lv2:symbol "Input_R";
    lv2:name "Input_R";
],
[
    a lv2:OutputPort, lv2:AudioPort;
    lv2:index 10;
    lv2:symbol "Output_L";
    lv2:name "Output_L";
],
[
    a lv2:OutputPort, lv2:AudioPort;
    lv2:index 11;
    lv2:symbol "Output_R";
    lv2:name "Output_R";
].
