@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://github.com/davemollen/dm-Shredmaster>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-dm-shredmaster.html> ;
        modgui:stylesheet <modgui/stylesheet-dm-shredmaster.css> ;
        modgui:screenshot <modgui/screenshot-dm-shredmaster.png> ;
        modgui:thumbnail <modgui/thumbnail-dm-shredmaster.png> ;
        modgui:javascript <modgui/javascript.js> ;
        modgui:brand "DM" ;
        modgui:label "Shredmaster" ;
        modgui:model "boxy" ;
        modgui:panel "5-knobs" ;
        modgui:color "black" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "gain" ;
            lv2:name "Gain" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "bass" ;
            lv2:name "Bass" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "contour" ;
            lv2:name "Contour" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "treble" ;
            lv2:name "Treble" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "volume" ;
            lv2:name "Volume" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "brilliance" ;
            lv2:name "Brilliance" ;
        ] ;
    ] .