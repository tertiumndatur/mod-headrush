@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://distrho.sf.net/plugins/MVerb>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-mverb.html> ;
        modgui:stylesheet <modgui/stylesheet-mverb.css> ;
        modgui:screenshot <modgui/screenshot-mverb.png> ;
        modgui:thumbnail <modgui/thumbnail-mverb.png> ;
        modgui:brand "DISTRHO" ;
        modgui:label "MVerb" ;
        modgui:model "japanese" ;
        modgui:panel "8-knobs" ;
        modgui:color "petrol" ;
        modgui:knob "yellow" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "predelay" ;
            lv2:name "Predelay" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "density" ;
            lv2:name "Density" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "bandwidth" ;
            lv2:name "Bandwidth" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "decay" ;
            lv2:name "Decay" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "damping" ;
            lv2:name "Damping" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "size" ;
            lv2:name "Size" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "gain" ;
            lv2:name "Level" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "mix" ;
            lv2:name "Mix" ;
        ] ;
    ] .