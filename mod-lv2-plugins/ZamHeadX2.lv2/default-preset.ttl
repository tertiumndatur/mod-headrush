@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <urn:zamaudio:ZamHeadX2> ;
	lv2:port [
		lv2:symbol "az" ;
		pset:value 0.0
	] , [
		lv2:symbol "elev" ;
		pset:value 0.0
	] , [
		lv2:symbol "width" ;
		pset:value 1.0
	] .

