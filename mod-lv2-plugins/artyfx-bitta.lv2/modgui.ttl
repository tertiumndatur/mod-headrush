@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .
@prefix modgui: <http://moddevices.com/ns/modgui#> .

<http://www.openavproductions.com/artyfx#bitta>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-bitta.html> ;
        modgui:stylesheet <modgui/stylesheet-bitta.css> ;
        modgui:screenshot <modgui/screenshot-bitta.png> ;
        modgui:thumbnail <modgui/thumbnail-bitta.png> ;
        modgui:brand "OpenAV" ;
        modgui:label "Bitta" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "crush" ;
            lv2:name "Crush" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "drywet" ;
            lv2:name "Mix" ;
        ] ;
    ] .
