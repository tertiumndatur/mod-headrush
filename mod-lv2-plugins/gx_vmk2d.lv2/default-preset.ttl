@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://guitarix.sourceforge.net/plugins/gx_vmk2d_#_vmk2d_> ;
	lv2:port [
		lv2:symbol "BASS" ;
		pset:value 0.5
	] , [
		lv2:symbol "DEPTH" ;
		pset:value 0.5
	] , [
		lv2:symbol "MRB" ;
		pset:value 0.0
	] , [
		lv2:symbol "MRBSELECT" ;
		pset:value 0.0
	] , [
		lv2:symbol "REVERB" ;
		pset:value 0.0
	] , [
		lv2:symbol "REVERBLEVEL" ;
		pset:value 0.5
	] , [
		lv2:symbol "SPEED" ;
		pset:value 0.5
	] , [
		lv2:symbol "TREBLE" ;
		pset:value 0.5
	] , [
		lv2:symbol "VIBE" ;
		pset:value 0.0
	] , [
		lv2:symbol "VOLUME" ;
		pset:value 0.5
	] .

