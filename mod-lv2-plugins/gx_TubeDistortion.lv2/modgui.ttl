@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://guitarix.sourceforge.net/plugins/gx_TubeDistortion_#_TubeDistortion_>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-gxtubedistortion.html> ;
        modgui:stylesheet <modgui/stylesheet-gxtubedistortion.css> ;
        modgui:screenshot <modgui/screenshot-gxtubedistortion.png> ;
        modgui:thumbnail <modgui/thumbnail-gxtubedistortion.png> ;
        modgui:brand "Guitarix" ;
        modgui:label "GxTubeDistortion" ;
        modgui:model "boxy" ;
        modgui:panel "2-knobs" ;
        modgui:color "tubedistortion" ;
        modgui:knob "cairo" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "INPUT" ;
            lv2:name "INPUT" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "OUTPUT" ;
            lv2:name "OUTPUT" ;
        ] ;
    ] .
