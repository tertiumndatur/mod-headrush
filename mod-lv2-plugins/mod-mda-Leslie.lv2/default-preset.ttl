@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://moddevices.com/plugins/mda/Leslie> ;
	lv2:port [
		lv2:symbol "hi_depth" ;
		pset:value 70.0
	] , [
		lv2:symbol "hi_throb" ;
		pset:value 70.0
	] , [
		lv2:symbol "hi_width" ;
		pset:value 70.0
	] , [
		lv2:symbol "lo_throb" ;
		pset:value 60.0
	] , [
		lv2:symbol "lo_width" ;
		pset:value 50.0
	] , [
		lv2:symbol "mode" ;
		pset:value 0.5
	] , [
		lv2:symbol "output" ;
		pset:value 0.0
	] , [
		lv2:symbol "speed" ;
		pset:value 100.0
	] , [
		lv2:symbol "x_over" ;
		pset:value 772.8
	] .

