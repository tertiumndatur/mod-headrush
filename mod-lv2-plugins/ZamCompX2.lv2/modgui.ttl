@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<urn:zamaudio:ZamCompX2>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-zamcompx2.html> ;
        modgui:stylesheet <modgui/stylesheet-zamcompx2.css> ;
        modgui:screenshot <modgui/screenshot-zamcompx2.png> ;
        modgui:thumbnail <modgui/thumbnail-zamcompx2.png> ;
        modgui:brand "ZamAudio" ;
        modgui:label "ZamCompX2" ;
        modgui:model "boxy" ;
        modgui:panel "7-knobs" ;
        modgui:color "cream" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "thr" ;
            lv2:name "Thresh" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "rat" ;
            lv2:name "Ratio" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "kn" ;
            lv2:name "Knee" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "slew" ;
            lv2:name "Slew" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "att" ;
            lv2:name "Attack" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "rel" ;
            lv2:name "Release" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "mak" ;
            lv2:name "Makeup" ;
        ] ;
    ] .