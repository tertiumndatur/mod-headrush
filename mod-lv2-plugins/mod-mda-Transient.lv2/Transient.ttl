@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix pg: <http://lv2plug.in/ns/ext/port-groups#> .
@prefix mda: <http://moddevices.com/plugins/mda/> .
@prefix mod: <http://moddevices.com/ns/mod#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix epp: <http://lv2plug.in/ns/ext/port-props#>.
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix pprops: <http://lv2plug.in/ns/ext/port-props#> .

mda:Transient
    a lv2:Plugin ,
        lv2:DynamicsPlugin ;
    lv2:project mda: ;
    lv2:symbol "Transient" ;
    doap:name "MDA Transient" ;
    doap:license <http://usefulinc.com/doap/licenses/gpl> ;
    rdfs:comment "MDA Transient" ;
    lv2:optionalFeature lv2:hardRTCapable ;
    pg:mainInput mda:mainIn ;
    pg:mainOutput mda:mainOut ;
    lv2:minorVersion 0;
    lv2:microVersion 1;
    mod:brand "MDA" ;
    mod:label "Transient" ;
    lv2:port [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 0 ;
        lv2:name "Attack" ;
        lv2:symbol "attack" ;
        lv2:default 50.0 ;
        lv2:minimum -100.0 ;
        lv2:maximum 100.0 ;
        units:unit units:pc
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 1 ;
        lv2:name "Release" ;
        lv2:symbol "release" ;
        lv2:default 50.0 ;
        lv2:minimum -100.0 ;
        lv2:maximum 100.0 ;
        units:unit units:pc
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 2 ;
        lv2:name "Output" ;
        lv2:symbol "output" ;
        lv2:default 0.0 ;
        lv2:minimum -20.0 ;
        lv2:maximum 20.0 ;
        units:unit units:db
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 3 ;
        lv2:name "Filter" ;
        lv2:symbol "filter" ;
        lv2:default 0.0 ;
        lv2:minimum -10.0 ;
        lv2:maximum 10.0 ;
        units:unit [
        a            units:Unit ;
        rdfs:label   "Low/High" ;
        units:symbol "Lo<>Hi" ;
        units:render "%f Lo<>Hi"
        ]
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 4 ;
        lv2:name "Att Hold" ;
        lv2:symbol "att_hold" ;
        lv2:default 35.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 100.0;
        units:unit units:pc
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 5 ;
        lv2:name "Rel Hold" ;
        lv2:symbol "rel_hold" ;
        lv2:default 35.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 100.0 ;
        units:unit units:pc
    ] , [
        a lv2:InputPort ,
            lv2:AudioPort ;
        lv2:index 6 ;
        lv2:symbol "left_in" ;
        lv2:name "Left In" ;
        lv2:designation pg:left ;
        pg:group mda:mainIn
    ] , [
        a lv2:InputPort ,
            lv2:AudioPort ;
        lv2:index 7 ;
        lv2:symbol "right_in" ;
        lv2:name "Right In" ;
        lv2:designation pg:right ;
        pg:group mda:mainIn
    ] , [
        a lv2:OutputPort ,
            lv2:AudioPort ;
        lv2:index 8 ;
        lv2:symbol "left_out" ;
        lv2:name "Left Out" ;
        lv2:designation pg:left ;
        pg:group mda:mainOut
    ] , [
        a lv2:OutputPort ,
            lv2:AudioPort ;
        lv2:index 9 ;
        lv2:symbol "right_out" ;
        lv2:name "Right Out" ;
        lv2:designation pg:right ;
        pg:group mda:mainOut
    ] .
