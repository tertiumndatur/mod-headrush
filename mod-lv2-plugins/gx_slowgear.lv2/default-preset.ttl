@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://guitarix.sourceforge.net/plugins/gx_slowgear_#_slowgear_> ;
	lv2:port [
		lv2:symbol "DOWNTIME" ;
		pset:value 5.0
	] , [
		lv2:symbol "TRESHOLD" ;
		pset:value 1.0
	] , [
		lv2:symbol "UPTIME" ;
		pset:value 100.0
	] .

