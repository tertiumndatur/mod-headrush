@prefix doap:  <http://usefulinc.com/ns/doap#>.
@prefix epp:   <http://lv2plug.in/ns/ext/port-props#>.
@prefix foaf:  <http://xmlns.com/foaf/0.1/>.
@prefix lv2:   <http://lv2plug.in/ns/lv2core#>.
@prefix mod:   <http://moddevices.com/ns/mod#>.
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#>.
@prefix units: <http://lv2plug.in/ns/extensions/units#>.

<http://moddevices.com/plugins/tap/chorusflanger>
a lv2:Plugin, lv2:ModulatorPlugin;

doap:name "TAP Chorus/Flanger";

doap:developer [
    foaf:name "Tom Szilagyi";
    foaf:homepage <http://tap-plugins.sourceforge.net/>;
    foaf:mbox <mailto:tsziagyi@users.sourceforge.net>;
];

doap:maintainer [
    foaf:name "MOD Team";
    foaf:homepage <http://moddevices.com>;
    foaf:mbox <mailto:devel@moddevices.com>;
];

mod:brand "TAP";
mod:label "Chorus/Flanger";

doap:license <http://usefulinc.com/doap/licenses/gpl>;
doap:wiki <http://tap-plugins.sourceforge.net/ladspa/chorusflanger.html>;

lv2:optionalFeature lv2:hardRTCapable;

lv2:minorVersion 7;
lv2:microVersion 2;

rdfs:comment """
This plugin is an implementation capable of creating traditional Chorus and Flanger effects, spiced up a bit to make use of stereo processing.
It sounds best on guitar and synth tracks.
""";

lv2:port
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 0;
    lv2:symbol "Frequency";
    lv2:name "Frequency";
    lv2:default 1.75;
    lv2:minimum 0.0;
    lv2:maximum 5.0;
    units:unit units:hz;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 1;
    lv2:symbol "LRPhaseShift";
    lv2:name "L/R Phase Shift";
    lv2:default 90.0;
    lv2:minimum 0.0;
    lv2:maximum 180.0;
    units:unit units:degree;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 2;
    lv2:symbol "Depth";
    lv2:name "Depth";
    lv2:default 75.0;
    lv2:minimum 0.0;
    lv2:maximum 100.0;
    units:unit units:pc;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 3;
    lv2:symbol "Delay";
    lv2:name "Delay";
    lv2:default 25.0;
    lv2:minimum 0.0;
    lv2:maximum 100.0;
    units:unit units:ms;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 4;
    lv2:symbol "Contour";
    lv2:name "Contour";
    lv2:default 100.0;
    lv2:minimum 20.0;
    lv2:maximum 20000.0;
    lv2:portProperty epp:logarithmic;
    units:unit units:hz;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 5;
    lv2:symbol "DryLevel";
    lv2:name "Dry Level";
    lv2:default -3.0;
    lv2:minimum -90.0;
    lv2:maximum 20.0;
    units:unit units:db;
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 6;
    lv2:symbol "WetLevel";
    lv2:name "Wet Level";
    lv2:default -3.0;
    lv2:minimum -90.0;
    lv2:maximum 20.0;
    units:unit units:db;
],
[
    a lv2:InputPort, lv2:AudioPort;
    lv2:index 7;
    lv2:symbol "InputL";
    lv2:name "Input L";
],
[
    a lv2:InputPort, lv2:AudioPort;
    lv2:index 8;
    lv2:symbol "InputR";
    lv2:name "Input R";
],
[
    a lv2:OutputPort, lv2:AudioPort;
    lv2:index 9;
    lv2:symbol "OutputL";
    lv2:name "Output L";
],
[
    a lv2:OutputPort, lv2:AudioPort;
    lv2:index 10;
    lv2:symbol "OutputR";
    lv2:name "Output R";
].
