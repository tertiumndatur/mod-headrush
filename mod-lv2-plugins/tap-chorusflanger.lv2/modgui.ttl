@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://moddevices.com/plugins/tap/chorusflanger>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-tap-chorusflanger.html> ;
        modgui:stylesheet <modgui/stylesheet-tap-chorusflanger.css> ;
        modgui:screenshot <modgui/screenshot-tap-chorusflanger.png> ;
        modgui:thumbnail <modgui/thumbnail-tap-chorusflanger.png> ;
        modgui:brand "TAP" ;
        modgui:label "Chorus/Flanger" ;
        modgui:model "japanese" ;
        modgui:panel "7-a-knobs" ;
        modgui:color "purple" ;
        modgui:knob "purple" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "Depth" ;
            lv2:name "Depth" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "Frequency" ;
            lv2:name "Rate" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "WetLevel" ;
            lv2:name "Wet" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "Delay" ;
            lv2:name "Delay" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "LRPhaseShift" ;
            lv2:name "L/R Phase Shift" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "Contour" ;
            lv2:name "Contour" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "DryLevel" ;
            lv2:name "Dry" ;
        ] ;
    ] .