@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://www.niallmoody.com/ndcplugs/ampimposer.htm> ;
	lv2:port [
		lv2:symbol "depth" ;
		pset:value 1.0
	] , [
		lv2:symbol "thres" ;
		pset:value 0.5
	] .

