@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://moddevices.com/plugins/mda/RePsycho>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-mda-repsycho.html> ;
        modgui:stylesheet <modgui/stylesheet-mda-repsycho.css> ;
        modgui:screenshot <modgui/screenshot-mda-repsycho.png> ;
        modgui:thumbnail <modgui/thumbnail-mda-repsycho.png> ;
        modgui:brand "MDA" ;
        modgui:label "RePsycho" ;
        modgui:model "boxy" ;
        modgui:panel "6-knobs" ;
        modgui:color "slime" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "tune" ;
            lv2:name "Tune" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "fine" ;
            lv2:name "Fine" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "mix" ;
            lv2:name "Mix" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "thresh" ;
            lv2:name "Thresh" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "hold" ;
            lv2:name "Hold" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "decay" ;
            lv2:name "Decay" ;
        ] ;
    ] .