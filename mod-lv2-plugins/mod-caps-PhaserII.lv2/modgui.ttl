@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://moddevices.com/plugins/caps/PhaserII>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-c-phaserii-mono-phaser-modulated-by-a-lorenz-fractal.html> ;
        modgui:stylesheet <modgui/stylesheet-c-phaserii-mono-phaser-modulated-by-a-lorenz-fractal.css> ;
        modgui:screenshot <modgui/screenshot-c-phaserii-mono-phaser-modulated-by-a-lorenz-fractal.png> ;
        modgui:thumbnail <modgui/thumbnail-c-phaserii-mono-phaser-modulated-by-a-lorenz-fractal.png> ;
        modgui:brand "CAPS" ;
        modgui:label "Phaser II" ;
        modgui:model "boxy" ;
        modgui:panel "4-knobs" ;
        modgui:color "purple" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "depth" ;
            lv2:name "Depth" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "rate" ;
            lv2:name "Rate" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "resonance" ;
            lv2:name "Reso" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "spread" ;
            lv2:name "Spread" ;
        ] ;
    ] .