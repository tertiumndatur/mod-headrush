@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://guitarix.sourceforge.net/plugins/gx_mbecho_#_mbecho_> ;
	lv2:port [
		lv2:symbol "PERCENT3" ;
		pset:value 45.0
	] , [
		lv2:symbol "PERCENT5" ;
		pset:value 0.0
	] .

