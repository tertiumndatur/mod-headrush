@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://moddevices.com/plugins/mda/ThruZero> ;
	lv2:port [
		lv2:symbol "depth" ;
		pset:value 20.0
	] , [
		lv2:symbol "depth_mod" ;
		pset:value 100.0
	] , [
		lv2:symbol "feedback" ;
		pset:value -40.0
	] , [
		lv2:symbol "mix" ;
		pset:value 47.0
	] , [
		lv2:symbol "rate" ;
		pset:value 0.08
	] .

