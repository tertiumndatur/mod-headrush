@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://gareus.org/oss/lv2/b_whirl#simple> ;
	lv2:port [
		lv2:symbol "drumlvl" ;
		pset:value 0.0
	] , [
		lv2:symbol "drumwidth" ;
		pset:value 1.0
	] , [
		lv2:symbol "hornlvl" ;
		pset:value 0.0
	] , [
		lv2:symbol "rt_speed" ;
		pset:value 4.0
	] .

