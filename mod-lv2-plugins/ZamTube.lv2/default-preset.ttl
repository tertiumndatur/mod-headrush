@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <urn:zamaudio:ZamTube> ;
	lv2:port [
		lv2:symbol "bass" ;
		pset:value 0.5
	] , [
		lv2:symbol "gain" ;
		pset:value 0.0
	] , [
		lv2:symbol "insane" ;
		pset:value 0.0
	] , [
		lv2:symbol "mids" ;
		pset:value 0.5
	] , [
		lv2:symbol "tonestack" ;
		pset:value 0.0
	] , [
		lv2:symbol "treb" ;
		pset:value 0.0
	] , [
		lv2:symbol "tubedrive" ;
		pset:value 0.0
	] .

