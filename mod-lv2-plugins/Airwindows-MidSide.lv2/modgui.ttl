@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://hannesbraun.net/ns/lv2/airwindows/midside>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-midside.html> ;
        modgui:stylesheet <modgui/stylesheet-midside.css> ;
        modgui:screenshot <modgui/screenshot-midside.png> ;
        modgui:thumbnail <modgui/thumbnail-midside.png> ;
        modgui:brand "Hannes" ;
        modgui:label "MidSide" ;
        modgui:model "boxy" ;
        modgui:panel "1-knob" ;
        modgui:color "purple" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "midside" ;
            lv2:name "Mid/Side" ;
        ] ;
    ] .