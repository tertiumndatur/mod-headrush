@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://distrho.sf.net/plugins/PingPongPan> ;
	lv2:port [
		lv2:symbol "freq" ;
		pset:value 50.0
	] , [
		lv2:symbol "width" ;
		pset:value 75.0
	] .

